{
  "name": "joybot",
  "version": "0.0.0",
  "description": "a webscrapper and all around good guy for mafia phpbb forms",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "validate": "tslint --project tsconfig.json",
    "prod": "npm run validate && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mebird/joybot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "couchbase": "^3.0.1",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "lodash": "^4.17.15",
    "romanjs": "^2.0.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.18",
    "@types/couchbase": "^2.4.1",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.4",
    "jest": "^25.5.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src",
      "<rootDir>/src/jobs",
      "<rootDir>/src/jobs/commands"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(tsx?|ts?)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
